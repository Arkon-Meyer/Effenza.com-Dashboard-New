{
  "name": "effenza-dashboard",
  "version": "1.0.0",
  "description": "Effenza.com Admin Dashboard - MVP backend & UI",
  "main": "server.js",
  "type": "commonjs",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "migrate": "node scripts/migrate.js",
    "seed": "node scripts/seed-demo.js",
    "reset:demo": "node scripts/reset-demo.js",
    "smoke:audit": "bash scripts/smoke-audit.sh",
    "smoke:audit:detail": "bash scripts/smoke-audit-detail.sh",
    "audit:cleanup": "node scripts/cron-clean-audit.js",
    "postinstall": "npm run migrate && SEED_QUIET=1 npm run seed && if [ \"$POSTINSTALL_SKIP_SMOKE\" != \"1\" ]; then bash scripts/smoke-audit.sh --quiet; fi",
    "ci:lint:install": "bash scripts/install-actionlint.sh",
    "ci:lint": "bash -lc '[ -x .bin/actionlint ] || bash scripts/install-actionlint.sh; .bin/actionlint -color || true'"
  },
  "keywords": ["effenza", "dashboard", "admin", "express", "postgres"],
  "author": "Effenza Engineering <dev@effenza.com>",
  "license": "Proprietary",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "pg": "^8.12.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.3",
    "dotenv": "^16.4.5"
  },
  "engines": { "node": "20.x" }
}
